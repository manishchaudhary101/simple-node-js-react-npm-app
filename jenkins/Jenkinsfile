pipeline {
     agent any
         parameters {
        choice(
            choices: ['AUS' , 'EU', 'USA'],
            description: '',
            name: 'Choose Environment')
    }
   
     tools {nodejs "Node16"}
  
     stages {
        stage("Build") {
             input {
  message 'Choose the Environment'
}

            steps {
                sh "npm install"
                sh "npm run build"
            }
        }
        stage("Deploy") {
            steps {
                echo "deploying"
                
            }
        }
    }
}
